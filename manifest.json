{
  "manifest_version": 3,
  "name": "업무용 To-Do List",
  "version": "1.0.0",
  "description": "아이젠하워 매트릭스 기반 업무용 할 일 관리 앱",
  "icons": {
    "16": "icons/todo_icon_16x16.png",
    "32": "icons/todo_icon_32x32.png",
    "48": "icons/todo_icon_48x48.png",
    "128": "icons/todo_icon_128x128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "To-Do List"
  },
  "permissions": [
    "storage"
  ]
} 